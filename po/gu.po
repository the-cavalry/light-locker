msgstr "<span size=\"x-large\">%R</span>"

#: ../data/lock-dialog-default.ui.h:7
msgid "_Cancel"
msgstr "રદ કરો (_C)"

#: ../data/lock-dialog-default.ui.h:8
msgid "_Leave Message"
msgstr "સંદેશો છોડો (_L)"

#: ../data/lock-dialog-default.ui.h:9
msgid "_Log Out"
